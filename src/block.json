{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "jartes/wikiblock",
	"version": "0.1.0",
	"title": "Wikiblock",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block written with ESNext standard and JSX support â€“ build step required.",
	"supports": {
		"html": false
	},
	"attributes": {
		"mapUrl": {
			"type": "string",
			"default": ""
		},
		"mapType": {
			"type": "string", 
			"default": "H"
		},
		"mapWidth": {
			"type": "number",
			"default": 500
		},
		"mapHeight": {
			"type": "number",
			"default": 400
		},
		"showTitle": {
			"type": "boolean",
			"default": false
		},
		"showMeasuresElevation": {
			"type": "boolean",
			"default": false
		},
		"showLocation": {
			"type": "boolean",
			"default": false
		},
		"showImages": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "wikiblock",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}